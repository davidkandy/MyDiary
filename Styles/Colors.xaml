<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:MyDiary">
    
    <Color x:Key="LightDarkGreen">#99d9d9</Color>
    <SolidColorBrush x:Key="LightDarkGreenBrush" Color="{StaticResource LightDarkGreen}"/>

    <Color x:Key="DarkGrey">#000</Color>
    <SolidColorBrush x:Key="DarkGreyBrush" Color="{StaticResource DarkGrey}"/>
    
    <Color x:Key="LightWhite">#FFF</Color>
    <SolidColorBrush x:Key="LightWhiteBrush" Color="{StaticResource LightWhite}"/>

</ResourceDictionary>